class Solution {
    public void reverseString(char[] s) {
        // String str="";
        // for(char i:s)
        //     str+=i;
        // int n=str.length()-1;
        // for(int i=0;i<str.length();i++)
        //     s[i]=str.charAt(n--);
        int i,j;
        for(i=0, j=s.length-1; i<=j; i++, j--){
           char temp = s[i];
            s[i] = s[j];
            s[j] = temp;
        }
    }
}
